<div>
  <h1> Save any kind of movies </h1>
</div>

<div>
  <div>
    <h2>My lists</h2>
  </div>
  <p><%= link_to "Create a movie list", new_list_path, class:"create"%></p>

  <div class="container">
    <% @lists.each do |list| %>
      <div class="card-list"><p><%= link_to "#{list.name }", list_path(list) %></p></div>
    <% end %>
  </div>
</div>
